    "build-local": "NODE_ENV='local' webpack -p --progress",
    "build-local:watch": "NODE_ENV='local' webpack -p --progress --watch",
    "build-dev": "NODE_ENV='dev' webpack --progress",
    "build-dev:watch": "NODE_ENV='dev' webpack --progress --watch",
    "build-prod": "NODE_ENV='production' webpack --progress",
    "build-prod:watch": "NODE_ENV='production' webpack --progress --watch"
  },
  "devDependencies": {
    "@types/history": "*",
    "@types/lodash": "^4.14.61",
