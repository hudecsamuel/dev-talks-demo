{
  "name": "import-ant-client",
  "version": "1.1.0",
  "description": "Client side react application for FeedImageEditor",
  "private": true,
  "authors": [
    {
      "name": "Samuel Hudec",
      "company": "Reglendo Technologies"
    }
  ],
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "NODE_ENV='local' webpack-dev-server",
