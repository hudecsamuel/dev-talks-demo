    "typescript": "^2.2.2",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "axios": "^0.16.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "mergado-ui-kit": "^1.0.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
