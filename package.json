    "@types/react": "^15.0.21",
    "@types/react-dom": "^0.14.23",
    "@types/react-router": "^3.0.6",
    "@types/react-router-redux": "^5.0.0",
    "@types/redux-saga": "^0.10.5",
    "@types/webpack": "^2.2.14",
    "@types/webpack-dev-server": "^1.12.6",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-webpack-alias": "^2.1.2",
